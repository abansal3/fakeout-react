{
  "manifest_version": 2,
  "name": "Fakeout (React)",
  "description": "CrowdSourcing Truth in Social Media",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://localhost:3000"
  ]
}